<div class="container my-5">
	{{form_start(form)}}
	{{form_row(form.email)}}
	{{form_row(form.nom)}}
	{{form_row(form.prenom)}}
	{{form_row(form.entreprise)}}
	{{form_row(form.site_web)}}
	{% if is_granted('ROLE_ADMIN') %}
		{{form_row(form.activation)}}
	{% endif %}

	{{form_row(form.modifier)}}
	<div class="mb-3 row">
		<div class="col-sm-2"></div>
		<div class="col-sm-10">
			<a class="btn btn-danger" href="javascript:history.go(-1)" role="button">Annuler</a>
		</div>

	</div>
	{{ form_row(form._token) }}
	{{ form_end(form, {render_rest: false}) }}

</div>
