<div class="container my-5">
	<div class="row">
		<div class="col">
			{{form_start(form)}}
			{{form_row(form.email)}}
			{{form_row(form.password)}}
			{{form_row(form.nom)}}
			{{form_row(form.prenom)}}
			{{form_row(form.entreprise)}}
			{{form_row(form.site_web)}}


			{{form_row(form.modifier)}}</div>
	</div>
	<div class="mb-3 row">
		<div class="col-sm-2"></div>
		<div class="col-sm-10">
			<a class="btn btn-danger" href="javascript:history.go(-1)" role="button">Annuler</a>
		</div>

	</div>
	{{ form_row(form._token) }}
	{{ form_end(form, {render_rest: false}) }}

</div>
